int <-function(mod){ #
	int <- rep(coef(mod)[[1]], length(x2))#
	for (i in 2:length(x2)) { int[i] <- int[i] + coef(mod)[grepl(x2[i],#
		 names(coef(mod))) & !grepl("area", names(coef(mod)))]}#
	int} #
