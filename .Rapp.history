	slp <- rep(coef(mod)[[2]], length(x2))#
	for (i in 2:length(x2)) { slp[i] <- slp[i] + coef(mod)[grepl(x2[i],#
		names(coef(mod))) & grepl("area", names(coef(mod)))]}#
	slp} #
