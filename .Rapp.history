# repeat function to generate mesh for each species#
max.size <- 0.3 # max(ss$area)#
n <- 100#
mesh <- function(){#
	min.size <- rec.size # 3.5#
	b <- seq(min.size, max.size, length=n)#
	h <- b[2] - b[1]#
	b <- c(min(b)-(2*h), min(b)-h, b)#
	y <- 0.5 * (b[1:n]+b[2:(n+1)])#
	I <- y >= rec.size#
	return(list(b=b,h=h,y=y,I=I, rec.size = rec.size))}#
